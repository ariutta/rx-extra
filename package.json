{
  "name": "rx-extra",
  "version": "1.0.2",
  "description": "rx-extra contains additional functionality not contained in the rx package, such as \"splitOnChange\".",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/spec/api.js"
  },
  "browser": {
    "rx-node": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ariutta/rx-extra.git"
  },
  "keywords": [
    "rx",
    "rxjs",
    "observable"
  ],
  "author": "Anders Riutta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ariutta/rx-extra/issues"
  },
  "homepage": "https://github.com/ariutta/rx-extra#readme",
  "dependencies": {
    "inherits": "^2.0.1",
    "rx-node": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "jsonstream": "^1.0.3",
    "loggin": "^3.0.1",
    "mocha": "^2.3.4",
    "rx-fs": "^1.0.0-alpha.2",
    "sinon": "^1.17.2"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib"
      ]
    }
  }
